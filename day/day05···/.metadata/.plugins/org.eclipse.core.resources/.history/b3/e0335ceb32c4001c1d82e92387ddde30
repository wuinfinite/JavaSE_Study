package com.zipeng.java;

public class NetWorkTest {

}

interface NetWork{
	public void browse();//浏览
}

//被代理类
class Server implements NetWork{

	@Override
	public void browse() {
		System.out.println("真实的服务器访问网络");
		
	}
}  

//代理类
class ProxySever implements NetWork{
	
	public void check(){
		System.out.println("联网之前的检查工作	");
	}
	
	@Override
	public void browse() {
			check();
	}
	
}