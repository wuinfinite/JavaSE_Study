package com.zipeng.exer;

public class Person {
	
	String name;
	int age;
	int sex;
	/*
	 sex = 1  男性
	 sex = 0 女性
	 */
	
	public void study(){
		System.out.println("studying");
	}
	
	public void showAge(){
		System.out.println("age:" + age);
	}
	
	public int addAge(int i){
		age += i;
		return age;
	}

}
